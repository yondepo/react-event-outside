{
  "extends": "airbnb",
  "parser"  : "babel-eslint",
  "env": {
    "browser": true,
    "mocha": true
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-return-assign": 0
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true
  },
}
